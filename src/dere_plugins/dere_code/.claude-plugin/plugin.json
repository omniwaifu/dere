{
  "name": "dere-code",
  "description": "Coding workflow automation for dere - symbol-aware navigation, memory persistence, and safe refactoring patterns",
  "version": "1.4.0",
  "author": {
    "name": "dere"
  },
  "keywords": [
    "automation",
    "multi-agent",
    "research",
    "productivity",
    "workflow"
  ],
  "mcpServers": {
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "."
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    }
  }
}
